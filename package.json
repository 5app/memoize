{
	"name": "@5app/js-template",
	"version": "1.2.1",
	"private": true,
	"description": "Description",
	"main": "index.js",
	"scripts": {
		"lint": "eslint ./",
		"lint-fix": "npm run lint -- --fix",
		"lint-diff": "LIST=`git diff-index --name-only HEAD | grep '.*\\.js$';`; if [ \"$LIST\" ]; then eslint $LIST; fi",
		"precommit-msg": "echo 'Running pre-commit checks... (skip using --no-verify)' && exit 0",
		"semantic-release": "semantic-release",
		"test": "echo 'No tests' && exit 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/5app/js-template.git"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/5app/js-template/issues"
	},
	"homepage": "https://github.com/5app/js-template#readme",
	"devDependencies": {
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@semantic-release/changelog": "^5.0.0",
		"@semantic-release/git": "^9.0.0",
		"eslint": "^6.7.2",
		"eslint-config-5app": "^0.8.0",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-prettier": "^3.1.1",
		"husky": "^4.0.0",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"semantic-release": "^17.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"subject-case": [
				2,
				"never",
				[
					"start-case",
					"pascal-case"
				]
			]
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E  HUSKY_GIT_PARAMS",
			"pre-commit": "pretty-quick --staged && npm run precommit-msg && npm run lint-diff"
		}
	}
}
